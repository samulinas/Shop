@model IEnumerable<Shop.Models.Category>

@{
    ViewData["Title"] = "Index";
}

<h2>Category List</h2>
@if (Model.Count() > 0)
{
    @foreach (var obj in Model)
    {
        <div>@obj.Name</div>
        <div>@obj.DisplayOrder</div>
        <button>Редактировать</button>
        <button>Удалить</button>
        <br>
    }
}
else
{
    <p>Категорий не существует!</p>
}
